{
  "name": "estate-agent-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://THISANGAD.github.io/Estate-Agent-APP",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "react": "^19.2.3",
    "react-datepicker": "^9.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.3",
    "react-image-gallery": "^1.4.0",
    "react-router-dom": "^6.30.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.10.2",
    "react-tabs": "^6.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-dnd|react-dnd-html5-backend|dnd-core|@react-dnd)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/node_modules/react-scripts/config/jest/cssTransform.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "gh-pages": "^6.3.0"
  }
}
